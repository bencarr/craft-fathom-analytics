{
    "name": "bencarr/craft-fathom-analytics",
    "description": "An unofficial Fathom Analytics integration for Craft CMS",
    "type": "craft-plugin",
    "license": "mit",
    "support": {
        "issues": "https://github.com/bencarr/craft-fathom-analytics/issues?state=open",
        "source": "https://github.com/bencarr/craft-fathom-analytics",
        "docs": "https://github.com/bencarr/craft-fathom-analytics",
        "rss": "https://github.com/bencarr/craft-fathom-analytics/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.5.0",
        "putyourlightson/craft-sprig-core": "^2.6"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "bencarr\\fathom\\": "src/"
        }
    },
    "extra": {
        "handle": "fathom",
        "name": "Fathom Analytics",
        "developer": "Ben Carr",
        "documentationUrl": "https://github.com/bencarr/craft-fathom-analytics",
        "class": "bencarr\\fathom\\FathomPlugin"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
